{
  "name": "345SecurityMQTT",
  "version": "0.1.2",
  "slug": "345securitymqtt",
  "description": "345SecurityMQTT with Auto Discovery support. It listen's for events from 345MHz security sensors and sends messages via MQTT.",
  "url": "https://github.com/kdeyev/345SecurityMQTT",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "application",
  "boot": "auto",
  "services": ["mqtt:want"],
  "usb": true,
  "udev": true,
  "options": {
    "autodiscovery": true,
    "device_id": 0,
    "frequency": 345000000
  },
  "schema": {
    "autodiscovery": "bool",
    "device_id": "int",
    "frequency": "int"
  }
}
