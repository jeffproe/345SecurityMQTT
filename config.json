{
  "name": "345SecurityMQTT",
  "version": "0.2.4",
  "slug": "345securitymqtt",
  "description": "345SecurityMQTT with Auto Discovery support. It listens for events from 345MHz security sensors and sends messages via MQTT.",
  "url": "https://github.com/kdeyev/345SecurityMQTT",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "application",
  "boot": "auto",
  "services": ["mqtt:need"],
  "usb": true,
  "udev": true,
  "uart": true,
  "host_network": false,
  "hassio_api": true,
  "map": ["config:rw"],
  "options": {
    "autodiscovery": true,
    "device_id": 0,
    "frequency": 345000000,
    "gain": 700,
    "sample_rate": 1000000
  },
  "schema": {
    "autodiscovery": "bool",
    "device_id": "int",
    "frequency": "int",
    "gain": "int",
    "sample_rate": "int"
  }
}
